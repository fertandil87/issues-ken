<h1>UserAnswer</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Answer</th>
            <td>{{ entity.answer }}</td>
        </tr>
        <tr>
            <th>Responsetime</th>
            <td>{{ entity.responseTime }}</td>
        </tr>
        <tr>
            <th>Rightanswer</th>
            <td>{{ entity.rightAnswer }}</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('administrator') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('administrator_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('administrator_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
